#: main_1.cpp:98699
msgid "Select the lower rating range for an opponent"
msgstr "Chọn phạm vi xếp hạng thấp hơn cho đối thủ"

#: main_1.cpp:98700
msgid "Select the upper rating range for an opponent"
msgstr "Chọn phạm vi xếp hạng cao nhất cho đối thủ"

#: main_1.cpp:98701
msgid "Check to play a rated game"
msgstr "Kiểm tra để chơi trò chơi được xếp hạng"

#: main_1.cpp:98702
msgid "Check for automatic response to your game advert"
msgstr "Kiểm tra phản hồi tự động cho quảng cáo trò chơi của bạn"

#: main_1.cpp:98703
msgid "Check to apply your formula to filter game advert responses"
msgstr "Kiểm tra để áp dụng công thức của bạn để lọc phản hồi quảng cáo trò chơi"

#: main_1.cpp:98752
msgid "This dialog box allows you to enter the parameters for your game advert"
msgstr "Hộp thoại này cho phép bạn nhập các thông số cho quảng cáo trò chơi của bạn"

